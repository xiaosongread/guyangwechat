page{padding:90rpx 0 0;}
.tabTit{height:90rpx;line-height: 90rpx;border-bottom: solid 1px #ddd;position:fixed;top:0;width: 750rpx;z-index: 10;background: #EFEFF4;}
.tabTit .active{color:#fe3a35;background: #fff;}
.tabTit .active image{transform: rotate(180deg);background: none;}
.tabTit image{width:26rpx;height:26rpx;vertical-align: middle;margin-left: 5px;background:none;}
.tabLayer{box-shadow: 0 5px 5px rgba(0,0,0,.15);width:750rpx;overflow: hidden;position: fixed;top:90rpx;z-index: 10;background: #fff;padding-bottom: 26rpx;border-bottom: solid 1px #eee;}
.tabLayer text{width:210rpx;height:60rpx;line-height:60rpx;float:left;border: solid 1px #eee;margin:20rpx 0 0 26rpx;}
.tabLayer .active{color:#fe3a35;border-color:#fe3a35;}
.case{height:640rpx;margin:30rpx auto 0;background: #fff;border:solid 1px #f1f1f1;overflow:hidden;}
.case .title{font-size: 33rpx;color:#000;margin-top: 5rpx;}
.case .title,.case .tag{padding:0 5rem 0 15px;}
.case .tag{font-size:28rpx;}
.case .pic{width:100%;height:510rpx;}
.case .headPic{right:10px;top:455rpx;}
.case .headPic image{width:100rpx;height:100rpx;border-radius: 50%;border:solid 6rpx #fff;}
.case .headPic text{display: block;font-size:28rpx;margin-top: -6rpx;width: 4em;}

.favorite {background: rgba(0,0,0,.5);position: absolute;top:460rpx;width:100%;line-height: 50rpx;color:#fff;}
.favorite .pic_num{background:url(data:image/png;base64,) no-repeat left center;background-size:35rpx auto;display:inline-block;padding-left:40rpx;height:50rpx;margin-left: 30rpx;}
.favorite .top_num{background:url(data:image/png;base64,) no-repeat left center;background-size:35rpx auto;display:inline-block;padding-left:40rpx;height:50rpx;margin-left: 30rpx;}

.searchbar-result{
    margin-top: 0;
    font-size: 14px;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}